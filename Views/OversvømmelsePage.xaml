<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="HousePriceing.Views.OversvømmelsePage"
             Title="OversvømmelsePage">
    <StackLayout Padding="24" >
        <Image
            Source="{Binding OversvømmelsePicture}"
            HeightRequest="80"
            WidthRequest="80"
            Margin="0,70,0,0"
            
        />
        <Label
            Text="{Binding OversvømmelseText}"
            FontSize="Medium"
            Padding="0,77,0,0"
        />
    </StackLayout>
</ContentPage>