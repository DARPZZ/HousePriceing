<?xml version="1.0" encoding="UTF-8" ?>
<Shell
	x:Class="HousePriceing.AppShell"
	xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
	xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
	xmlns:views="clr-namespace:HousePriceing.Views"
	xmlns:local="clr-namespace:HousePriceing"
    >
    
    <FlyoutItem FlyoutIcon="hest.jpg"  Title="Hjem">
        <Tab>
           <ShellContent
              
               Title="Hjem"
               ContentTemplate="{DataTemplate views:MainPage}" 
               Route="MainPage"/>
        </Tab>
    </FlyoutItem>
    <FlyoutItem FlyoutIcon="hest.jpg"  Title="Bolig Opvarmning">
     <Tab>
        <ShellContent
           
            Title="Bolig Opvarmning"
            ContentTemplate="{DataTemplate views:BoligOpvarmningPage}" 
            Route="BoligOpvarmningPage"/>
     </Tab>
 </FlyoutItem>

    <FlyoutItem FlyoutIcon="hest.jpg"  Title="TrafikStøj">
        <Tab>
            <ShellContent
          
           Title="TrafikStøj"
           ContentTemplate="{DataTemplate views:TrafikStøjPage}" 
           Route="TrafikStøjPage"/>
        </Tab>
    </FlyoutItem>

</Shell>
